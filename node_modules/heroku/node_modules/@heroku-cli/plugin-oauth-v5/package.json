  },
  "dependencies": {
    "co": "^4.6.0",
    "date-fns": "^1.29.0",
    "heroku-cli-util": "^8.0.9",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.34",
    "@oclif/plugin-legacy": "^1.0.15",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "1.17.0",
    "nock": "9.3.3",
    "nyc": "^12.0.2",
    "standard": "11.0.1",
    "unexpected": "10.38.0"
  },
  "files": [
    "oclif.manifest.json",
    "commands",
    "lib"
  ],
  "homepage": "https://github.com/heroku/heroku-cli-oauth",
